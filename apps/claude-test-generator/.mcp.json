{
  "mcpServers": {
    "test-generator-github": {
      "command": "python3",
      "args": [".claude/mcp/simple_github_mcp_server.py"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    "test-generator-filesystem": {
      "command": "python3",
      "args": [".claude/mcp/minimal_filesystem_server.py"]
    },
    "n8n-mcp": {
      "command": "npx",
      "args": ["n8n-mcp@latest"]
    }
  }
}